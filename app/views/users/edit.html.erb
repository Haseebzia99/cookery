<div class="container">
  <div class="row justify-content-center">
    <div class="col-6">

      <h1>Add/Update Cuisine</h1>
      <%= simple_form_for current_user do |f| %>
        <%= f.input :cuisine %>
        <%= f.input :photo, as: :file %>
        <%= f.input :chef_photo, as: :file %>
        <%= f.submit class: 'btn btn-dark' %>
        
      <% end %>

    </div>
  </div>
</div>
